#GitHub
Host github.com
IdentityFile /Users/jcrown/.ssh/github_rsa

#MT Git Repo
Host git.mtsvc.net
IdentityFile ~/.ssh/cteng_id_rsa
##ControlMaster auto
##ControlPath /tmp/ssh_mux_%h_%p_%r
##ControlPersist 30s

##General MTSVC connections
Host *.mtsvc.net
User jcrown
IdentityFile ~/.ssh/cteng_id_rsa

##Dev Servers
Host 172.17.*.*
    User jcrown
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no

#Agent Forwarding
Host *
ForwardAgent yes
User jcrown
                   
Host 192.168.59.103
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
